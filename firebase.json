{
  "hosting": {
    "public": "www",
    "rewrites": [
      {
        "source": "/remote",
        "destination": "/remote.html"
      },
      {
        "source": "/r",
        "destination": "/remote.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
